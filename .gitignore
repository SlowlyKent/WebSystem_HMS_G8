# CodeIgniter 4
#--------------------------------------------------------------------
# .env file should not be committed
#--------------------------------------------------------------------
.env
.env.*
!.env.example

#--------------------------------------------------------------------
# Writable directories - these should not be tracked
#--------------------------------------------------------------------
writable/session/*
!writable/session/index.html
writable/cache/*
!writable/cache/index.html
writable/logs/*.log
!writable/logs/index.html
writable/debugbar/*
!writable/debugbar/index.html
writable/uploads/*
!writable/uploads/index.html

#--------------------------------------------------------------------
# Composer
#--------------------------------------------------------------------
/vendor/

#--------------------------------------------------------------------
# IDE
#--------------------------------------------------------------------
.idea/
.vscode/
*.swp
*.swo
*~

#--------------------------------------------------------------------
# OS
#--------------------------------------------------------------------
.DS_Store
Thumbs.db

#--------------------------------------------------------------------
# Build
#--------------------------------------------------------------------
build/
.phpunit.result.cache

